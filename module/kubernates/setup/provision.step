#!/usr/bin/env bash
blackbox.module.kubernates.setup.provision() {
  : <<< blackbox.framework.module.abstract.setup.provision:step

  blackbox.framework.inventory.provision k3s && {
    service k3s start
  }

  blackbox.framework.inventory.install.file script.sh </blackbox/module/kubernates/setup/provision/rootfs/home/%/%/script.sh
}
